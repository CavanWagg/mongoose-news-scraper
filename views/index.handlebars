<div class="jumbotron text-center" style="background-color: #7afcff">
<h1 class="">Quillette News Scraper!</h1>
<button class="btn"id="getArticles"><a style="color: white" href="/scrape">scrape for articles</a></button>
</div>

  <div id="wrapper">
    <div id="articles" class="card">
      {{#each dbArticle}}
      <div class="card-body">
        <a target="_blank" class="card-title" href={{link}}>{{title}}</a>
      <p class="card-text" data-id={{_id}}>{{summary}}</p>
      </div>
      {{/each}}
    </div>
     <div id="notes"></div>
  </div>
 
